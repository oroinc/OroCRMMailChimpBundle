{
    "name": "oro/crm-mail-chimp",
    "description": "OroCRM MailChimp integration",
    "homepage": "https://github.com/orocrm/OroCRMMailChimpBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\MailChimpBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/MailChimpBundle",
    "repositories": [
       {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.4.9",
        "oro/crm": "1.4.*",
        "zfr/zfr-mailchimp": "~2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
