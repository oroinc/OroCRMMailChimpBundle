{
    "name": "oro/crm-mail-chimp",
    "description": "OroCRM MailChimp integration",
    "homepage": "https://github.com/orocrm/OroCRMMailChimpBundle.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Oro\\Bundle\\MailChimpBundle": "src/"
        }
    },
    "target-dir": "src/Oro/Bundle/MailChimpBundle",
    "require": {
        "php": ">=5.6",
